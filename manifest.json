{
  "manifest_version": 2,

  "name": "Trello Clean",
  "description": "Get a clean board on Trello.com",
  "version": "0.1.0",
  "author": "Manuel Gruber",
  "homepage_url": "http://manuelgruber.com",
  "content_scripts": [
    {
      "matches": ["https://trello.com/*"],
      "css": ["/source/clean.css"],
      "run_at": "document_end"
    }
  ]
}
